if x2 < 0:\n        x2 = 0\n        x1 = 1\n    if x1 < 1:\n        x1 = 1\n    led.plot(x1, y1)\n    led.plot(x2, y1)\ninput.on_button_pressed(Button.A, on_button_pressed_a)\n\ndef on_button_pressed_b():\n    global x2, x1\n    led.unplot(x1, y1)\n    led.unplot(x2, y1)\n    x2 = x2 + 1\n    x1 = x1 + 1\n    if x2 > 3:\n        x2 = 3\n    if x1 > 4:\n        x1 = 4\n    led.plot(x1, y1)\n    led.plot(x2, y1)\ninput.on_button_pressed(Button.B, on_button_pressed_b)\n\ny2 = 0\nx2 = 0\nx1 = 0\ny1 = 0\nx2 = 1\nx1 = 2\ny1 = 4\nled.plot(x1, y1)\nled.plot(x2, y1)\nx3 = randint(1,3)\nled.plot(x3, y2)\nrichting_x = randint(-1, 1)\nrichting_y = 1\n\ndef on_forever():\n    global x3, y2, richting_x, richting_y\n    led.unplot(x3, y2)\n    x3 = x3 + richting_x\n    y2 = y2 + richting_y\n    led.plot(x3, y2)\n    basic.pause(300)\n    if x3 < 1:\n        led.unplot(x3, y2)\n        richting_x = 1\n        x3 = x3 + richting_x\n        y2 = y2 + richting_y\n        led.plot(x3, y2)\n        basic.pause(300)\n    if x3 > 3:\n        richting_x = -1\n        led.unplot(x3, y2)\n        x3 = x3 + richting_x\n        y2 = y2 + richting_y\n        led.plot(x3, y2)\n        basic.pause(300)\n    if y2 == 5:\n        basic.show_string(\"GAME OVER\")\n        led.unplot(x3, y2)\n        led.unplot(x2, y1) and [x1, y1]\n    if x3 == x1 and y2 == y1 or x3 == x2 and y2 == y1:\n        richting_y = -1\n        led.unplot(x3, y2)\n        richting_x = randint(-1, 1)\n        x3 = x3 + richting_x\n        y2 = y2 + richting_y\n        led.plot(x2, y1)\n        led.plot(x1, y1)\n        led.plot(x3, y2)\n        basic.pause(300)\nbasic.forever(on_forever)\n","pxt.json":"{\n    \"name\": \"pong\",\n    \"description\": \"\",\n    \"dependencies\": {\n        \"core\": \"*\",\n        \"radio\": \"*\",\n        \"microphone\": \"*\"\n    },\n    \"files\": [\n        \"main.blocks\",\n        \"main.ts\",\n        \"README.md\",\n        \"main.py\"\n    ],\n    \"preferredEditor\": \"pyprj\"\n}