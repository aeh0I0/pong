 > 3:\n        led.unplot(x3, y2)\n        richting_x = -1\n        x3 = x3 + richting_x\n        y2 = y2 + richting_y\n        led.plot(x3, y2)\n        basic.pause(300)\n    if y2 > 4:\n        basic.show_string(\"GAME OVER\")\n        led.unplot(x3, y2)\n        led.unplot(x2, y1) and [x1, y1]\n    if x3 == x1 and y2 == y1:\n        richting_y = -1\n        led.unplot(x3, y2)\n        richting_x = -1\n        x3 = x3 + richting_x\n        y2 = y2 + richting_y\n        led.plot(x2, y1)\n        led.plot(x1, y1)\n        led.plot(x3, y2)\n        basic.pause (200)\n        if x3 > 3:\n            led.unplot (x3,y2)\n            richting_x = -1\n            led.plot (x3,y2)\n            basic.pause(300)\n        if x3 < 1:\n            led.unplot (x3,y2)\n            richting_x = 1\n            led.plot(x3,y2)\n            basic.pause (300)    \nbasic.forever(on_forever)\n","pxt.json":"{\n    \"name\": \"pong\",\n    \"description\": \"\",\n    \"dependencies\": {\n        \"core\": \"*\",\n        \"radio\": \"*\",\n        \"microphone\": \"*\"\n    },\n    \"files\": [\n        \"main.blocks\",\n        \"main.ts\",\n        \"README.md\",\n        \"main.py\"\n    ],\n    \"preferredEditor\": \"pyprj\"\n}\n"}}],"shares":[],"lastSaveTime":1744120191213}